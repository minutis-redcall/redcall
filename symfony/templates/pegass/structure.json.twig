{
    "structure": {
        "id": "{{ structure.id }}",
        "typeStructure": "",
        "libelle": "{{ structure.label|raw }}",
        "libelleCourt": "{{ structure.short_label|raw }}",
        "adresse": "{{ structure.address|raw }}",
        "telephone": "",
        "mail": "",
        "parent": {
            "id": "{{ structure.parent_id }}"
        },
        "structureMenantActiviteList": [
            {
                "id": "{{ structure.id }}",
                "libelle": "{{ structure.label|raw }}"
            }
        ]
    },
    "responsible": {
        "responsableId": "{{ structure.responsible|raw }}"
    },
    "volunteers": [
        {
            "content": [
                {% for volunteer_id in structure.volunteer_ids %}
                {
                    "id": "{{ volunteer_id }}"
                }
                {% if not loop.last %},
                {% endif %}
                {% endfor %}
            ]
        }
    ]
}